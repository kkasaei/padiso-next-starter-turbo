{
  "name": "searchfit-turbo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "dotenv -e .env -- turbo build",
    "dev": "dotenv -e .env -- turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "db:generate": "dotenv -e .env -- turbo db:generate",
    "db:push": "dotenv -e .env -- turbo db:push",
    "db:seed": "dotenv -e .env -- turbo db:seed",
    "db:studio": "dotenv -e .env -- turbo db:studio",
    "db:reset": "dotenv -e .env -- turbo db:reset",
    "-------------------": "-----------------------",
    "stripe:listen": "stripe listen --forward-to localhost:3001/api/webhooks/stripe",
    "--------------------- Emails -----------------------------------------": "-",
    "emails:preview": "dotenv -e .env -- pnpm --filter @workspace/email emails:preview",
    "--------------Trigger.dev--------------": "--------------------------------",
    "trigger:dev": "dotenv -e .env -- pnpm --filter @workspace/trigger trigger:dev",
    "trigger:deploy": "dotenv -e .env -- pnpm --filter @workspace/trigger trigger:deploy",
    "trigger:deploy:staging": "dotenv -e .env -- pnpm --filter @workspace/trigger trigger:deploy:staging",
    "trigger:deploy:production": "dotenv -e .env -- pnpm --filter @workspace/trigger trigger:deploy:production",
    "--------------------- ClickHouse ----------------------------------------": "-",
    "build:embed": "pnpm build --filter embed",
    "killport": "npx kill-port 3000 3001 3002 3003"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "dotenv-cli": "^11.0.0",
    "prettier": "^3.7.4",
    "turbo": "^2.6.3",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.4.1",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "overrides": {
      "@radix-ui/react-tabs": "^1.1.2",
      "@radix-ui/react-direction": "^1.1.1",
      "@radix-ui/react-context": "^1.1.1",
      "@radix-ui/react-id": "^1.1.1",
      "@radix-ui/react-use-layout-effect": "^1.1.1",
      "@radix-ui/react-presence": "^1.1.2",
      "@radix-ui/react-compose-refs": "^1.1.1",
      "@radix-ui/react-primitive": "^2.0.1",
      "@radix-ui/react-slot": "^1.1.1",
      "@radix-ui/react-roving-focus": "^1.1.2",
      "@radix-ui/react-use-controllable-state": "^1.1.1",
      "@radix-ui/react-use-callback-ref": "^1.1.1",
      "@radix-ui/react-collection": "^1.1.1"
    }
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.88"
  }
}